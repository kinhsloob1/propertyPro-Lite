{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "entry", // or "usuage"
                "corejs": 3,
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "absoluteRuntime": false,
                "corejs": {
                    "version": 3,
                    "proposals": true
                },
                "helpers": true,
                "regenerator": true,
                "useESModules": false
            }
        ]
    ]
}